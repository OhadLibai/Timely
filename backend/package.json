{
  "name": "timely-backend",
  "version": "1.0.0",
  "description": "Timely ML Demo Backend API - Sanitized",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r ./src/alias.ts src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node -r ./dist/alias.js dist/server.js",
    "seed": "node -r ./dist/alias.js dist/database/database.seed.js",
    "seed:dev": "ts-node -r ./src/alias.ts src/database/database.seed.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "timely",
    "ml-demo",
    "ecommerce",
    "prediction",
    "express",
    "typescript"
  ],
  "_moduleAliases": {
    "@": "dist",
    "@/controllers": "dist/controllers",
    "@/models": "dist/models",
    "@/routes": "dist/routes",
    "@/services": "dist/services",
    "@/middleware": "dist/middleware",
    "@/utils": "dist/utils",
    "@/types": "dist/types",
    "@/config": "dist/config",
    "@/database": "dist/database",
    "@/validators": "dist/validators"
  }
}
